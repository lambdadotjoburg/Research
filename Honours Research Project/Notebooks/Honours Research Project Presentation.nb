Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalGamma]", "(", "z", ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], " ", 
     RowBox[{
      SuperscriptBox["t", 
       RowBox[{"z", "-", "1"}]], " ", 
      SuperscriptBox["e", 
       RowBox[{"-", "t"}]], " ", 
      RowBox[{"\[DifferentialD]", "t"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"b51c5ddd-e5c0-451f-86c5-2abea8105f77"],
 "\t\tand\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalGamma]", "(", 
     FractionBox["1", "2"], ")"}], "=", 
    SqrtBox["\[Pi]"]}], TraditionalForm]],ExpressionUUID->
  "919a488b-525b-4a63-9f60-6a52a81b479e"],
 " \tto derive\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["d", 
       FractionBox["1", "2"]], 
      RowBox[{"d", " ", 
       SuperscriptBox["x", 
        FractionBox["1", "2"]]}]], "x"}], "=", 
    RowBox[{"2", 
     SqrtBox[
      FractionBox["x", "\[Pi]"]]}]}], TraditionalForm]],ExpressionUUID->
  "d6568753-7c4f-40e7-8384-714394e6a074"],
 "\t\tand \t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["d", 
       FractionBox["1", "2"]], 
      RowBox[{"d", " ", 
       SuperscriptBox["x", 
        FractionBox["1", "2"]]}]], 
     SuperscriptBox["x", "0"]}], "=", 
    FractionBox["1", 
     SqrtBox[
      RowBox[{"\[Pi]", " ", "x"}]]]}], TraditionalForm]],ExpressionUUID->
  "b8dd7c9f-e85e-4e0f-b5ff-38099e40f941"],
 "\t\t(not 0)"
}], "Text",
