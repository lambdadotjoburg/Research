Cell[CellGroupData[{

Cell["Example (Use of Laplace)", "Subtitle",
 CellChangeTimes->{{3.7527333706185265`*^9, 3.75273347016022*^9}, {
  3.752733925692275*^9, 
  3.7527339563420277`*^9}},ExpressionUUID->"d34cecca-ebe6-4355-8a41-\
6e3b9cb32545"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["J", "\[Alpha]"], 
     RowBox[{"f", "(", " ", "x", " ", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ScriptCapitalL]", 
       RowBox[{"-", "1"}]], 
      RowBox[{"{", " ", 
       RowBox[{
        SuperscriptBox["s", 
         RowBox[{"-", "\[Alpha]"}]], "\[ScriptCapitalL]", 
        RowBox[{"{", " ", 
         SuperscriptBox["t", "k"], " ", "}"}]}], " ", "}"}]}], "=", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ScriptCapitalL]", 
        RowBox[{"-", "1"}]], 
       RowBox[{"{", 
        FractionBox[
         RowBox[{
          StyleBox["\[CapitalGamma]",
           FontSlant->"Plain"], 
          StyleBox[" ",
           FontSlant->"Plain"], 
          RowBox[{
           StyleBox["(",
            FontSlant->"Italic"], 
           StyleBox[" ",
            FontSlant->"Italic"], 
           RowBox[{
            StyleBox["k",
             FontSlant->"Italic"], 
            StyleBox[" ",
             FontSlant->"Italic"], "+", " ", "1"}], " ", 
           StyleBox[")",
            FontSlant->"Italic"]}]}], 
         SuperscriptBox["s", 
          RowBox[{"\[Alpha]", " ", "+", " ", "k", " ", "+", " ", "1"}]]], 
        "}"}]}], "=", 
      RowBox[{
       FractionBox[
        RowBox[{
         StyleBox["\[CapitalGamma]",
          FontSlant->"Plain"], 
         StyleBox[" ",
          FontSlant->"Plain"], 
         RowBox[{
          StyleBox["(",
           FontSlant->"Italic"], 
          StyleBox[" ",
           FontSlant->"Italic"], 
          RowBox[{
           StyleBox["k",
            FontSlant->"Italic"], 
           StyleBox[" ",
            FontSlant->"Italic"], "+", " ", "1"}], " ", 
          StyleBox[")",
           FontSlant->"Italic"]}]}], 
        StyleBox[
         RowBox[{"\[CapitalGamma]", "(", "  ", 
          RowBox[{"\[Alpha]", " ", "+", " ", "k", " ", "+", " ", "1"}], " ", 
          ")"}],
         FontSlant->"Plain"]], 
       SuperscriptBox["t", 
        RowBox[{"\[Alpha]", " ", "+", " ", "k"}]]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"8ce81869-0c89-4a1c-999e-1e74d0f350cd"],
 "\t\twhere ",
 StyleBox["\[Alpha] + k > -1",
  FontSlant->"Italic"],
 "\n"
}], "Text",
 CellChangeTimes->{{3.7527334855320992`*^9, 3.7527336042258883`*^9}, {
  3.7527338611325827`*^9, 3.7527338677749624`*^9}, {3.752733969616787*^9, 
  3.75273406757839*^9}, {3.7528365099396906`*^9, 
  3.752836560000554*^9}},ExpressionUUID->"ff21b076-a677-430a-9fe3-\
3a5586fa840b"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Gr\[UDoubleDot]nwald-Letnikov Formalism", "Subtitle",
 CellChangeTimes->{{3.752427273907892*^9, 3.7524272783781476`*^9}, 
   3.7527324970975637`*^9, 3.7527325689066715`*^9, 3.752733347928229*^9, {
   3.7527334750234985`*^9, 
   3.7527334773646317`*^9}},ExpressionUUID->"08fce535-1fad-4da6-aa31-\
79c5fe83269d"],

Cell[TextData[{
 "In contrast to the Riemann-Liouville formalism which derives a \
differ-integral from repeated integration, Gr\[UDoubleDot]nwald-Letnikov \
derives a differ-integral from repeated differentiation.\n\nThe classical \
definition of the derivative is given by \n",
 StyleBox["\n",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", "'"], "(", "x", ")"}], "=", 
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"h", "\[RightArrow]", "0"}]], 
     FractionBox[
      RowBox[{
       RowBox[{"f", "(", " ", 
        RowBox[{"x", " ", "+", " ", "h"}], " ", ")"}], " ", "-", " ", 
       RowBox[{"f", "(", " ", "x", " ", ")"}]}], "h"]}]}], TraditionalForm]],
  ExpressionUUID->"6c280ab6-5f98-4eb8-acf2-6220d59f7401"],
 StyleBox["\n\n",
  FontSlant->"Italic"],
 "by applying this definition to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", "'"], "(", "x", ")"}], TraditionalForm]],
  ExpressionUUID->"255ad62a-c564-4330-8834-ef2b0a761e63"],
 " we obtain",
 StyleBox["\n\n",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", "''"], "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["lim", 
       RowBox[{"h", "\[RightArrow]", "0"}]], 
      FractionBox[
       RowBox[{
        RowBox[{
         SuperscriptBox["f", "'"], "(", " ", 
         RowBox[{"x", " ", "+", " ", "h"}], " ", ")"}], " ", "-", " ", 
        RowBox[{
         SuperscriptBox["f", "'"], "(", " ", "x", " ", ")"}]}], "h"]}], "=", 
     RowBox[{
      RowBox[{
       UnderscriptBox["lim", 
        RowBox[{"h", "\[RightArrow]", "0"}]], 
       FractionBox[
        RowBox[{" ", 
         RowBox[{
          RowBox[{
           UnderscriptBox["lim", 
            RowBox[{"h", "\[RightArrow]", "0"}]], 
           FractionBox[
            RowBox[{
             RowBox[{"f", "(", " ", 
              RowBox[{"x", " ", "+", " ", "h", " ", "+", "h"}], ")"}], " ", 
             "-", " ", 
             RowBox[{"f", "(", " ", 
              RowBox[{"x", "+", "h"}], " ", ")"}]}], "h"]}], " ", "-", " ", 
          RowBox[{
           UnderscriptBox["lim", 
            RowBox[{"h", "\[RightArrow]", "0"}]], 
           FractionBox[
            RowBox[{
             RowBox[{"f", "(", " ", 
              RowBox[{"x", " ", "+", " ", "h"}], " ", ")"}], " ", "-", " ", 
             RowBox[{"f", "(", " ", "x", " ", ")"}]}], "h"], " "}]}]}], 
        "h"]}], "=", 
      RowBox[{
       UnderscriptBox["lim", 
        RowBox[{"h", "\[RightArrow]", "0"}]], 
       FractionBox[
        RowBox[{
         RowBox[{"f", "(", " ", 
          RowBox[{"x", " ", "+", " ", 
           RowBox[{"2", "h"}]}], " ", ")"}], " ", "-", "  ", 
         RowBox[{"2", 
          RowBox[{"f", "(", 
           RowBox[{"x", "+", "h"}], ")"}]}], "  ", "+", "  ", 
         RowBox[{"f", "(", " ", "x", " ", ")"}]}], 
        SuperscriptBox["h", "2"]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "bb401531-c4d7-4f41-9823-fd3807d04854"],
 StyleBox["\n\n",
  FontSlant->"Italic"],
 "Recursively, after n differentiations, we obtain the formula",
 StyleBox["\n\n",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", "n"], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", "n"]}]], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"h", "\[RightArrow]", "0"}]], " ", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["h", "n"]], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"m", "=", "0"}], "n"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "m"], 
        RowBox[{"(", GridBox[{
           {"n"},
           {"m"}
          }], ")"}], "  ", 
        RowBox[{"f", "(", " ", 
         RowBox[{"x", " ", "-", " ", 
          RowBox[{"m", " ", "h"}]}], " ", ")"}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"8952e87a-a9e0-4dc0-b060-d4aa120df4df"],
 StyleBox["\n\n",
  FontSlant->"Italic"],
 "This definition is generalized to non-integer values for n provided we \
substitute the binomial coefficients with their gamma coefficients and the \
upper limit of summation n goes to infinity as ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"t", "-", "a"}], "h"], TraditionalForm]],ExpressionUUID->
  "44c66f8e-0428-4590-840d-be613b0e4e0f"],
 "where t and a are the upper and lower limits of differentiation.",
 StyleBox["\n\n",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", "q"], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", "q"]}]], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"h", "\[RightArrow]", "0"}]], " ", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["h", "q"]], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"m", "=", "0"}], 
        FractionBox[
         RowBox[{"t", "-", "a"}], "h"]], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "m"], 
        FractionBox[
         RowBox[{"\[CapitalGamma]", "(", " ", 
          RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], 
         RowBox[{
          RowBox[{"m", "!"}], " ", 
          RowBox[{"\[CapitalGamma]", "(", 
           RowBox[{"q", " ", "-", " ", "m", " ", "+", "1"}], " ", ")"}]}]], 
        RowBox[{"f", "(", " ", 
         RowBox[{"x", " ", "-", " ", 
          RowBox[{"m", " ", "h"}]}], " ", ")"}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"e7fc88f7-f871-46be-9efd-1807a9d195be"],
 StyleBox["\n\n",
  FontSlant->"Italic"],
 "Similarly, the fractional order Gr\[UDoubleDot]nwald-Letnikov integral is \
derived by replacing q  by -q to obtain ",
 StyleBox["\n\n",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", 
       RowBox[{"-", " ", "q"}]], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["x", 
        RowBox[{"-", " ", "q"}]]}]], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"h", "\[RightArrow]", "0"}]], " ", 
     RowBox[{
      SuperscriptBox["h", "q"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"m", "=", "0"}], 
        FractionBox[
         RowBox[{"t", "-", "a"}], "h"]], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "m"], 
        FractionBox[
         RowBox[{"\[CapitalGamma]", "(", " ", 
          RowBox[{"q", " ", "+", " ", "m"}], " ", ")"}], 
         RowBox[{
          RowBox[{"m", "!"}], " ", 
          RowBox[{"\[CapitalGamma]", "(", " ", "q", " ", ")"}]}]], 
        RowBox[{"f", "(", " ", 
         RowBox[{"x", " ", "-", " ", 
          RowBox[{"m", " ", "h"}]}], " ", ")"}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"be3c8c2a-d466-495b-b900-ccf5a8063177"],
 StyleBox["\n\n",
  FontSlant->"Italic"],
 "Remarks:",
 StyleBox["\n\n",
  FontSlant->"Italic"],
 "It can be shown that the Gr\[UDoubleDot]nwald-Letnikov formalism is \
equivalent to the Riemann-Liouville formalism (however the proof is quite \
lengthy and won\[CloseCurlyQuote]t be discussed)\n\nThe \
Gr\[UDoubleDot]nwald-Letnikov formulation lends itself readily to numerical \
methods",
 StyleBox["\n",
  FontSlant->"Italic"]
}], "Text",
 CellChangeTimes->{{3.7524272855445576`*^9, 3.752427291893921*^9}, {
  3.752459459200782*^9, 3.752459476617778*^9}, {3.752459654223936*^9, 
  3.752459657763139*^9}, {3.752655284428557*^9, 3.752655297453302*^9}, {
  3.75265533465843*^9, 3.7526554949045954`*^9}, {3.752655598364513*^9, 
  3.752655641554983*^9}, {3.75266762977567*^9, 3.752667697014516*^9}, {
  3.7526677474764023`*^9, 3.752667776129041*^9}, {3.752667807698847*^9, 
  3.7526678560216107`*^9}, {3.7526679549732704`*^9, 3.752667956571362*^9}, {
  3.75273229352092*^9, 3.7527323144561176`*^9}, {3.752732485438897*^9, 
  3.752732490347178*^9}, {3.7527325350797367`*^9, 3.7527325377398887`*^9}, {
  3.7528336727264113`*^9, 
  3.752833691176467*^9}},ExpressionUUID->"6de725d8-a522-44a6-96f8-\
68107569b4ef"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Caputo Formalism", "Subtitle",
 CellChangeTimes->{{3.752427345471985*^9, 3.7524273510033016`*^9}, {
  3.752655543683386*^9, 3.752655587019864*^9}, {3.7527324734382105`*^9, 
  3.752732499269688*^9}},ExpressionUUID->"35403f43-70a3-42c7-b0e1-\
f61e8aa1bcc1"],

Cell[TextData[{
 "Recall that the Riemann-Liouville derivative was defined\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[InvisiblePrefixScriptBase]", 
      SuperscriptBox["D", "\[Alpha]"]}], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[DifferentialD]", 
         RowBox[{"\[DifferentialD]", "x"}]], ")"}], "m"], "[", 
      RowBox[{
       RowBox[{"\[InvisiblePrefixScriptBase]", 
        SuperscriptBox["D", 
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{"m", "-", "\[Alpha]"}], ")"}]}]]}], 
       RowBox[{"f", "(", "x", ")"}]}], "]"}], "=", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[DifferentialD]", 
         RowBox[{"\[DifferentialD]", "x"}]], ")"}], "m"], "[", 
      RowBox[{
       RowBox[{"\[InvisiblePrefixScriptBase]", 
        SuperscriptBox["J", 
         RowBox[{"(", 
          RowBox[{"m", "-", "\[Alpha]"}], ")"}]]}], 
       RowBox[{"f", "(", "x", ")"}]}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"84e5af8f-3884-4954-93a0-0b98ec65974d"],
 "\t\twhere the integer order derivative is applied second which produces \
transient terms\n\nand ",
 StyleBox["m",
  FontSlant->"Italic"],
 " is the nearest integer greater than \[Alpha]  i.e. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "=", 
    RowBox[{"Ceiling", "[", "\[Alpha]", "]"}]}], TraditionalForm]],
  ExpressionUUID->"8672df6d-8a1d-48ca-baf0-a9b20f180b36"],
 ", sometimes written as ",
 StyleBox["m - 1 < \[Alpha]  < m",
  FontSlant->"Italic"],
 "\n"
}], "Text",
 CellChangeTimes->{{3.7524273595667915`*^9, 3.7524274095026474`*^9}, 
   3.7526555326357536`*^9, 3.7526562154358077`*^9, {3.752667960139566*^9, 
