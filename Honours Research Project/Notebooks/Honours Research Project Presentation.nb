Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"We", " ", "write", " ", "the", " ", "expression", " ", "for", " ", 
    FormBox[
     RowBox[{
      RowBox[{"\[InvisiblePrefixScriptBase]", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["J", "\[Alpha]"], 
         SuperscriptBox["J", "\[Beta]"], "f"}], ")"}]}], 
      RowBox[{"(", "x", ")"}]}],
     TraditionalForm], " ", "using", " ", "the", " ", "definition", " ", "of",
     " ", "the", " ", "Riemann"}], "-", 
   RowBox[{"Liouville", " ", "integral", " ", 
    RowBox[{"operator", ".", "\n", "\n", 
     FormBox[
      RowBox[{
       RowBox[{
        RowBox[{"\[InvisiblePrefixScriptBase]", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["J", "\[Alpha]"], 
           RowBox[{"\[InvisiblePrefixScriptBase]", 
            SuperscriptBox["J", "\[Beta]"]}], "f"}], ")"}]}], 
        RowBox[{"(", "x", ")"}]}], "=", 
       RowBox[{
        FractionBox["1", 
         RowBox[{
          RowBox[{"\[CapitalGamma]", "(", "\[Alpha]", ")"}], 
          RowBox[{"\[CapitalGamma]", "(", "\[Beta]", ")"}]}]], 
        RowBox[{
         SubsuperscriptBox["\[Integral]", "a", "x"], 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "a", "t"], 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"x", "-", "t"}], ")"}], 
            RowBox[{"\[Alpha]", "-", "1"}]], 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"t", "-", "s"}], ")"}], 
            RowBox[{"\[Beta]", "-", "1"}]], 
           RowBox[{"f", "(", "s", ")"}], "  ", 
           RowBox[{"\[DifferentialD]", "s"}], "  ", 
           RowBox[{"\[DifferentialD]", "t"}]}]}]}]}]}],
      TraditionalForm]}], "\t", "where", " ", "the", " ", "double", " ", 
    "integral", " ", "is", " ", "taken", " ", "over", " ", "the", " ", 
    "region", " ", 
    FormBox[
     RowBox[{"R", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"s", ",", "t"}], ")"}], "|", 
        RowBox[{"a", "\[LessEqual]", "s", "\[LessEqual]", 
         RowBox[{"t", "  ", "and", "  ", "a"}], "\[LessEqual]", "t", 
         "\[LessEqual]", "x"}]}], "}"}]}],
     TraditionalForm]}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"Next", ",", " ", 
   RowBox[{
   "we", " ", "change", " ", "the", " ", "order", " ", "of", " ", 
    "integration", " ", "to", " ", "obtain"}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   FormBox[
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[InvisiblePrefixScriptBase]", 
         SuperscriptBox["J", "\[Alpha]"]}], 
        RowBox[{"\[InvisiblePrefixScriptBase]", 
         SuperscriptBox["J", "\[Beta]"]}], "f"}], ")"}], 
      RowBox[{"(", "x", ")"}]}], "=", 
     RowBox[{
      FractionBox["1", 
       RowBox[{
        RowBox[{"\[CapitalGamma]", "(", "\[Alpha]", ")"}], 
        RowBox[{"\[CapitalGamma]", "(", "\[Beta]", ")"}]}]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "a", "x"], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "s", "t"], 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"x", "-", "t"}], ")"}], 
          RowBox[{"\[Alpha]", "-", "1"}]], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"t", "-", "s"}], ")"}], 
          RowBox[{"\[Beta]", "-", "1"}]], 
         RowBox[{"f", "(", "s", ")"}], "  ", 
         RowBox[{"\[DifferentialD]", "s"}], "  ", 
         RowBox[{"\[DifferentialD]", "t"}]}]}]}]}]}],
    TraditionalForm], "\t", "where", " ", "the", " ", "region", " ", "is", 
   " ", "now", " ", "expressible", " ", "as", " ", 
   FormBox[
    RowBox[{
     RowBox[{"R", "'"}], "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"s", ",", "t"}], ")"}], "|", 
       RowBox[{"s", "\[LessEqual]", "t", "\[LessEqual]", 
        RowBox[{"x", "  ", "and", "  ", "a"}], "\[LessEqual]", "s", 
        "\[LessEqual]", "t"}]}], "}"}]}],
    TraditionalForm]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
  "We", " ", "then", " ", "introduce", " ", "the", " ", "change", " ", "of", 
   " ", "variables", " ", 
   FormBox[
    RowBox[{"t", "=", 
     RowBox[{"s", "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"x", "-", "s"}], ")"}], "r"}]}]}],
    TraditionalForm]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
   "After", " ", "manipulation", " ", "and", " ", "use", " ", "of", " ", 
    "the", " ", "beta", " ", "function"}], ",", " ", 
   RowBox[{"we", " ", "arrive", " ", "at", " ", "our", " ", "result"}]}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[InvisiblePrefixScriptBase]", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["J", "\[Alpha]"], 
      RowBox[{"\[InvisiblePrefixScriptBase]", 
       SuperscriptBox["J", "\[Beta]"]}], "f"}], ")"}]}], 
   RowBox[{"(", "x", ")"}]}], "=", 
