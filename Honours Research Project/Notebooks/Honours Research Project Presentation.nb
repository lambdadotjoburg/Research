adbc6a421197"],

Cell[TextData[{
 "The semi-derivative ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[InvisiblePrefixScriptBase]", 
    SuperscriptBox["D", 
     FractionBox["1", "2"]]}], TraditionalForm]],ExpressionUUID->
  "edcc173e-8c37-42e2-8ae8-6c1a3f69192b"],
 " of functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "t", ")"}], "=", "c"}], TraditionalForm]],
  ExpressionUUID->"d99f9b5e-4941-4687-9d53-5b514d835a96"],
 " where ",
 StyleBox["c",
  FontSlant->"Italic"],
 " is an arbitrary constant\n\nAgain, we use the more general case ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox["\[DifferentialD]", "\[Alpha]"], 
       RowBox[{"\[DifferentialD]", 
        SuperscriptBox["t", "\[Alpha]"]}]], "c"}], " ", 
     SuperscriptBox["t", "\[Lambda]"]}], "=", 
    RowBox[{"c", 
     FractionBox[
      RowBox[{
       StyleBox["\[CapitalGamma]",
        FontSlant->"Plain"], 
       StyleBox["(",
        FontSlant->"Italic"], 
       RowBox[{"\[Lambda]", "+", "1"}], 
       StyleBox[")",
        FontSlant->"Italic"]}], 
      StyleBox[
       RowBox[{"\[CapitalGamma]", "(", 
        RowBox[{"\[Lambda]", "-", "\[Alpha]", "+", "1"}], ")"}],
       FontSlant->"Plain"]], 
     SuperscriptBox["t", 
      RowBox[{"\[Lambda]", "-", "\[Alpha]"}]]}]}], TraditionalForm]],
  ExpressionUUID->"521105e9-eab7-4147-ac37-f110bdb264b8"],
 " and then apply the limit as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "\[RightArrow]", "0"}], TraditionalForm]],
  ExpressionUUID->"b8213fe8-ffa5-47d7-a9e0-0d7777f781a6"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["D", "\[Alpha]"], "c"}], " ", "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["lim", 
       RowBox[{"\[Lambda]", "\[RightArrow]", "0"}]], 
      RowBox[{
       SuperscriptBox["D", "\[Alpha]"], "c", " ", 
       SuperscriptBox["t", "\[Lambda]"]}]}], "=", 
     RowBox[{
      RowBox[{"c", 
       RowBox[{
        UnderscriptBox["lim", 
         RowBox[{"\[Lambda]", "\[RightArrow]", "0"}]], 
        RowBox[{
         FractionBox[
          RowBox[{
           StyleBox["\[CapitalGamma]",
            FontSlant->"Plain"], 
           StyleBox["(",
            FontSlant->"Italic"], 
           RowBox[{"\[Lambda]", "+", "1"}], 
           StyleBox[")",
            FontSlant->"Italic"]}], 
          StyleBox[
           RowBox[{"\[CapitalGamma]", "(", 
            RowBox[{"\[Lambda]", "-", "\[Alpha]", "+", "1"}], ")"}],
           FontSlant->"Plain"]], 
         SuperscriptBox["t", 
          RowBox[{"\[Lambda]", "-", "\[Alpha]"}]]}]}]}], "=", 
      RowBox[{
       FractionBox["c", 
        StyleBox[
         RowBox[{"\[CapitalGamma]", "(", 
          RowBox[{"1", "-", "\[Alpha]"}], ")"}],
         FontSlant->"Plain"]], 
       SuperscriptBox["t", 
        RowBox[{"-", "\[Alpha]"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "435bd7d3-1417-449d-af45-2dab6883a03f"]
}], "Text",
 CellChangeTimes->{{3.7524002134771237`*^9, 3.7524002150562143`*^9}, {
   3.752400690602414*^9, 3.7524006917144775`*^9}, 3.752400914950246*^9, {
