  FormBox[
   RowBox[{"k", "=", 
    FractionBox["1", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "72578884-d830-4804-a90f-e6647c7d03a6"],
