Cell[CellGroupData[{

Cell["Variations", "Section",
 CellChangeTimes->{{3.752225120291946*^9, 3.7522251612182865`*^9}, {
   3.752238700413847*^9, 3.752238728543456*^9}, {3.7522387982434425`*^9, 
   3.752238802113664*^9}, 3.7522404859059715`*^9, {3.752250109958436*^9, 
   3.752250113982666*^9}},ExpressionUUID->"0ca316b3-dc3a-4fb1-a547-\
c258827704f5"],

Cell[TextData[{
 "We would like to demonstrate, although not elaborate, that equally suitable \
alternative formalisms for the fractional calculus exists\n\n\nLet us suppose \
that the Riemann-Liouville fractional derivative is defined from the \
fractional integral by replacing \[Alpha], the order of integration, by -\
\[Alpha] to denote differentiation.\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[InvisiblePrefixScriptBase]", 
      SuperscriptBox["D", "\[Alpha]"]}], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["J", 
       RowBox[{"-", "\[Alpha]"}]], 
      RowBox[{"f", "(", "x", ")"}]}], "=", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"\[CapitalGamma]", "(", 
        RowBox[{"-", "\[Alpha]"}], ")"}]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "a", "x"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"x", "-", "t"}], ")"}], 
         RowBox[{
          RowBox[{"-", "\[Alpha]"}], "-", "1"}]], 
        RowBox[{"f", "(", "t", ")"}], 
        RowBox[{"\[DifferentialD]", "t"}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"7ddf1705-cf95-478e-906d-0329413e5866"],
 "\t\t(This is similar to the Weyl (1917) differ-integral)\n\n\nWe reconcile \
this approach with the Cauchy Integral formula from complex analysis\n\n\
Starting with\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", "n"], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["z", "n"]}]], 
     RowBox[{"f", "(", "z", ")"}]}], " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
     RowBox[{
      SubscriptBox["\[ContourIntegral]", "C"], 
      RowBox[{
       FractionBox[
        RowBox[{"f", "(", "\[Xi]", ")"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Xi]", "-", "z"}], ")"}], 
         RowBox[{"n", "+", "1"}]]], "d\[Xi]"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"4fb3b13a-753c-47b0-8065-369bb19118c0"],
 "\t\twhere ",
 StyleBox["C",
  FontSlant->"Italic"],
 " is a closed contour surrounding point ",
 StyleBox["z",
  FontSlant->"Italic"],
 " and enclosing a region of analyticity of ",
 StyleBox["f\n",
  FontSlant->"Italic"],
 "\nWe replace the positive integer ",
 StyleBox["n",
  FontSlant->"Italic"],
 " by a non-integer ",
 StyleBox["q",
  FontSlant->"Italic"],
 ", so that ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"\[Xi]", "-", "z"}], ")"}], 
    RowBox[{
     RowBox[{"-", "q"}], "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "2ac2d717-d0f2-49e0-a644-f791ed0a6cd9"],
 " no longer has a pole at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Xi]", "=", "z"}], TraditionalForm]],ExpressionUUID->
  "f17dff1f-912a-4bd0-84cb-f914ccd62eb0"],
 ", but a branch point so that we can define in the quadrant ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Re", "(", "\[Xi]", ")"}], " ", "\[LessEqual]", 
    RowBox[{"0", "   ", "and", "   ", 
     RowBox[{"Im", "(", "\[Xi]", ")"}]}], "\[LessEqual]", "0"}], 
   TraditionalForm]],ExpressionUUID->"a6d3c22f-ef75-4c5b-9042-8b93bb651092"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[DifferentialD]", "q"], 
      RowBox[{"\[DifferentialD]", 
       SuperscriptBox["z", "q"]}]], 
     RowBox[{"f", "(", "z", ")"}]}], " ", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[CapitalGamma]", "(", " ", 
       RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], 
      RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
     RowBox[{
      SubscriptBox["\[ContourIntegral]", "C"], 
      RowBox[{
       FractionBox[
        RowBox[{"f", "(", "\[Xi]", ")"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Xi]", "-", "z"}], ")"}], 
         RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"61f20704-920c-4165-b90d-13376dc3ac32"],
 "\t;\t",
 StyleBox["q",
  FontSlant->"Italic"],
 " not a negative integer\n\nwhere contour ",
 StyleBox["C",
  FontSlant->"Italic"],
 " begins and terminates at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Xi]", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "13e6f0f5-a604-4ee0-946a-5ccbcecdb76b"],
 ".\n\n\nwe will need the symmetry formula for the gamma function\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[CapitalGamma]", "(", " ", "z", " ", ")"}], " ", 
     RowBox[{"\[CapitalGamma]", "(", " ", 
      RowBox[{"1", " ", "-", " ", "z"}], " ", ")"}]}], " ", "=", 
    FractionBox["\[Pi]", 
     RowBox[{"sin", " ", 
      RowBox[{"(", " ", 
       RowBox[{"z", " ", "\[Pi]"}], " ", ")"}]}]]}], TraditionalForm]],
  ExpressionUUID->"be418cbb-1e45-4e4b-a8a6-b21791853224"],
 "  for   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"0", "<", 
     RowBox[{"Re", "(", " ", "z", " ", ")"}], "<", "1"}], ",", " ", 
    RowBox[{"z", " ", "not", " ", "an", " ", "integer"}]}], TraditionalForm]],
  ExpressionUUID->"432f91c1-2b3d-4764-b2e8-ae7a7fee65d3"],
 " and replacing ",
 StyleBox["z",
  FontSlant->"Italic"],
 " by ",
 StyleBox["q + 1 ",
  FontSlant->"Italic"],
 "to get \n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[CapitalGamma]", "(", " ", 
      RowBox[{"q", " ", "+", " ", "1"}], ")"}], " ", 
     RowBox[{"\[CapitalGamma]", "(", " ", 
      RowBox[{"-", " ", "q"}], " ", ")"}]}], " ", "=", 
    RowBox[{
     RowBox[{
      FractionBox["\[Pi]", 
       RowBox[{"sin", " ", 
        RowBox[{"(", " ", 
         RowBox[{
          RowBox[{"(", " ", 
           RowBox[{"q", "  ", "+", " ", "1"}], " ", ")"}], " ", "\[Pi]"}], 
         " ", ")"}]}]], " ", "\[LongRightArrow]", "  ", 
      RowBox[{"\[CapitalGamma]", "(", " ", 
       RowBox[{"q", " ", "+", " ", "1"}], ")"}]}], " ", "=", 
     FractionBox["\[Pi]", 
      RowBox[{
       RowBox[{"-", " ", "sin"}], " ", 
       RowBox[{"(", " ", 
        RowBox[{"q", " ", "\[Pi]"}], " ", ")"}], 
       RowBox[{"\[CapitalGamma]", "(", " ", 
        RowBox[{"-", " ", "q"}], " ", ")"}]}]]}]}], TraditionalForm]],
  ExpressionUUID->"4518b22e-8205-4b4d-9bd9-f90820120d4e"],
 "\n\n\nWe deform the contour to obtain an appropriate (Hankel) contour ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["C", "'"], TraditionalForm]],ExpressionUUID->
  "560e1db1-6397-4c01-8291-7adb13b19a30"],
 " about the point ",
 StyleBox["z",
  FontSlant->"Italic"],
 " and introduce the change of variables ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Xi]", "-", "z"}], ")"}], " ", "\[LongRightArrow]", " ", 
     RowBox[{"(", 
      RowBox[{"\[Xi]", "-", "z"}], ")"}]}], "  ", 
    SuperscriptBox["e", 
     RowBox[{"2", "iz"}]]}], TraditionalForm]],ExpressionUUID->
  "8bb0bc33-4cdf-4ac6-bd15-6372a303b6dd"],
 " so that\n\n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox["\[DifferentialD]", "q"], 
       RowBox[{"\[DifferentialD]", 
        SuperscriptBox["z", "q"]}]], 
      RowBox[{"f", "(", "z", ")"}]}], " ", "=", 
     RowBox[{
      FractionBox[
       RowBox[{"\[CapitalGamma]", "(", " ", 
        RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], 
       RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{"-", "2"}], "\[Pi]", " ", "i", " ", 
          RowBox[{"(", " ", 
           RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}]}]]}], ")"}], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "z"], 
       RowBox[{
        FractionBox[
         RowBox[{"f", "(", "\[Xi]", ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Xi]", "-", "z"}], ")"}], 
          RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]"}]}]}]}], 
    "\[IndentingNewLine]"}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t      ", 
    RowBox[{"=", " ", 
     RowBox[{
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"q", "+", "1"}], ")"}], 
      FractionBox[
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "\[Pi]"}], " ", "i", " ", 
         RowBox[{"(", " ", 
          RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}]}]], "\[Pi]"], 
      FractionBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["e", 
          RowBox[{"\[Pi]", " ", "i", " ", 
           RowBox[{"(", " ", 
            RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}]}]], " ", "-", " ", 
         SuperscriptBox["e", 
          RowBox[{
           RowBox[{"-", " ", "\[Pi]"}], " ", "i", " ", 
           RowBox[{"(", " ", 
            RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}]}]]}], ")"}], 
       RowBox[{"2", "  ", "\[ImaginaryI]"}]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "z"], 
       RowBox[{
        FractionBox[
         RowBox[{"f", "(", "\[Xi]", ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Xi]", "-", "z"}], ")"}], 
          RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]"}]}]}]}], 
    "\[IndentingNewLine]"}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t      ", 
    RowBox[{"=", " ", 
     RowBox[{
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"q", "+", "1"}], ")"}], 
      FractionBox[
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "\[Pi]"}], " ", "i", " ", 
         RowBox[{"(", 
          RowBox[{"q", "+", "1"}], ")"}]}]], "\[Pi]"], 
      RowBox[{"sin", "(", " ", 
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], " ", "\[Pi]"}], " ", 
       ")"}], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "z"], 
       RowBox[{
        FractionBox[
         RowBox[{"f", "(", "\[Xi]", ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Xi]", "-", "z"}], ")"}], 
          RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]"}]}]}]}], 
    "\[IndentingNewLine]"}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t      ", 
    RowBox[{"=", " ", 
     RowBox[{
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"q", "+", "1"}], ")"}], 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", " ", 
         RowBox[{"-", " ", "1"}], " ", ")"}], 
        RowBox[{
         RowBox[{"-", "q"}], "-", "1"}]], "\[Pi]"], 
      RowBox[{"sin", "(", " ", 
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], " ", "\[Pi]"}], " ", 
       ")"}], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "z"], 
       RowBox[{
        FractionBox[
         RowBox[{"f", "(", "\[Xi]", ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Xi]", "-", "z"}], ")"}], 
          RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]"}]}]}]}], 
    "\[IndentingNewLine]"}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t      ", 
    RowBox[{"=", 
     RowBox[{
      FractionBox["\[Pi]", 
       RowBox[{"sin", " ", 
        RowBox[{"(", " ", 
         RowBox[{
          RowBox[{"(", " ", 
           RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], " ", "\[Pi]"}], " ",
          ")"}], " ", 
        RowBox[{"\[CapitalGamma]", "(", " ", 
         RowBox[{"-", " ", "q"}], " ", ")"}]}]], 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", " ", 
         RowBox[{"-", " ", "1"}], " ", ")"}], 
        RowBox[{
         RowBox[{"-", "q"}], "-", "1"}]], "\[Pi]"], 
      RowBox[{"sin", "(", " ", 
       RowBox[{
        RowBox[{"(", " ", 
         RowBox[{"q", " ", "+", " ", "1"}], " ", ")"}], " ", "\[Pi]"}], " ", 
       ")"}], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "z"], 
       RowBox[{
        FractionBox[
         RowBox[{"f", "(", "\[Xi]", ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[Xi]", "-", "z"}], ")"}], 
          RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]"}]}]}]}], 
    "\[IndentingNewLine]"}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t     ", 
    RowBox[{"=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        RowBox[{"\[CapitalGamma]", "(", " ", 
         RowBox[{"-", " ", "q"}], " ", ")"}]], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "z"], 
        RowBox[{
         FractionBox[
          RowBox[{"f", "(", "\[Xi]", ")"}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"\[Xi]", "-", "z"}], ")"}], 
           RowBox[{"q", " ", "+", " ", "1"}]]], "d\[Xi]", "                       ",
          "Provided", "   ", 
         StyleBox["Re",
          FontSlant->"Italic"], 
         StyleBox[" ",
          FontSlant->"Italic"], 
         StyleBox[
          RowBox[{"(", " ", "q", " ", ")"}],
          FontSlant->"Italic"]}]}]}], 
      StyleBox["<",
       FontSlant->"Italic"], "0"}]}]}], TraditionalForm]}],ExpressionUUID->
  "3f78eebe-b908-48ff-aa5b-f1c0afbf97a9"],
 "\n\nThis is exactly the formula above with lower limit of integration ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "c31e0276-3892-45b3-913d-0ef35dcdcebe"],
 "\n"
}], "Text",
