Cell[CellGroupData[{

Cell["Definition (Caputo Derivative)", "Subtitle",
 CellChangeTimes->{{3.7526555136556683`*^9, 3.7526555176608973`*^9}, {
  3.7527324543421183`*^9, 
  3.752732476367378*^9}},ExpressionUUID->"8268e4f2-152c-4595-a7bc-\
8f68a825291b"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[InvisiblePrefixScriptBase]", 
      SuperscriptBox["D", "\[Alpha]"]}], 
     RowBox[{"f", "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["D", 
       RowBox[{"-", 
        RowBox[{"(", 
         RowBox[{"m", "-", "\[Alpha]"}], ")"}]}]], "[", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox["\[DifferentialD]", 
          RowBox[{"\[DifferentialD]", "x"}]], ")"}], "m"], 
       RowBox[{
        RowBox[{"\[InvisiblePrefixScriptBase]", "f"}], "(", "x", ")"}]}], 
      "]"}], "=", 
     RowBox[{
      SuperscriptBox["J", 
       RowBox[{"(", 
        RowBox[{"m", "-", "\[Alpha]"}], ")"}]], "[", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox["\[DifferentialD]", 
          RowBox[{"\[DifferentialD]", "x"}]], ")"}], "m"], 
       RowBox[{
        RowBox[{"\[InvisiblePrefixScriptBase]", "f"}], "(", "x", ")"}]}], 
      "]"}]}]}], TraditionalForm]],ExpressionUUID->
  "cdb8f7ca-8cd3-4466-bf8f-929f3aea497e"],
 "\n\nwhere ",
 StyleBox["m",
  FontSlant->"Italic"],
 " is the nearest integer greater than \[Alpha]  i.e. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "=", 
    RowBox[{"Ceiling", "[", "\[Alpha]", "]"}]}], TraditionalForm]],
  ExpressionUUID->"7ee62acc-38b0-48eb-aa85-41c239f090bf"],
 ", sometimes written as ",
 StyleBox["m - 1 < \[Alpha]  < m",
  FontSlant->"Italic"],
 "\n\nAs a result of integration second, the transient terms become in the \
form of a \[OpenCurlyDoubleQuote]series\[CloseCurlyDoubleQuote]. This leads \
us to the next discussion of the Fractional Taylor Series.\n"
}], "Text",
 CellChangeTimes->{
  3.752655535414912*^9, {3.7527324453376036`*^9, 
   3.752732469437982*^9}},ExpressionUUID->"56081d3a-b8b8-4642-af25-\
8a878a699f62"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

