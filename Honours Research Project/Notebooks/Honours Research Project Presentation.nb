     RowBox[{"n", "=", "1"}], "n"], 
    SuperscriptBox["i", "k"]}], "=", 
   RowBox[{"n", "+", "1"}]}]],ExpressionUUID->
  "e5a9ff3b-3eb0-46bb-8087-7974c3be5659"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", 
    RowBox[{"1", "          ", "\[DoubleLongRightArrow]", "          "}]}], 
   TraditionalForm]],ExpressionUUID->"6e1ba6ed-5231-47a5-bedf-c2c5476ee4ff"],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "1"}], "n"], 
    SuperscriptBox["i", "k"]}], "=", 
   FractionBox[
    RowBox[{"n", 
     RowBox[{"(", " ", 
      RowBox[{"n", " ", "+", " ", "1"}], " ", ")"}]}], "2"]}]],
  ExpressionUUID->"ebe49810-4bc5-4315-a353-587f837a0606"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", 
    RowBox[{"2", "         ", "\[DoubleLongRightArrow]", "          "}]}], 
   TraditionalForm]],ExpressionUUID->"d315d4cc-b189-4399-8aef-ba5e24a1193a"],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "1"}], "n"], 
    SuperscriptBox["i", "k"]}], "=", 
   FractionBox[
    RowBox[{"n", 
     RowBox[{"(", " ", 
      RowBox[{"n", " ", "+", " ", "1"}], " ", ")"}], 
     RowBox[{"(", " ", 
      RowBox[{
       RowBox[{"2", " ", "n"}], " ", "+", " ", "1"}], " ", ")"}]}], "6"]}]],
  ExpressionUUID->"6c781a04-39f1-4803-a897-b19e879d9c52"],
 "\n\nand so we are interested in an interpolation formula to generalise this \
power sum. Euler\[CloseCurlyQuote]s operator ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"x", 
     FractionBox["d", 
      RowBox[{"d", " ", "x"}]]}], ")"}], TraditionalForm]],ExpressionUUID->
  "e32d0dac-a0d5-481a-aecc-6f8de758cad1"],
 " has been used to establish\n\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "1"}], "n"], 
    SuperscriptBox["i", "k"]}], "=", 
   RowBox[{
    UnderscriptBox["lim", 
     RowBox[{"x", "\[RightArrow]", "1"}]], " ", 
    FormBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"x", 
         FractionBox["d", 
          RowBox[{"d", " ", "x"}]]}], ")"}], "k"], 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         SuperscriptBox["x", 
          RowBox[{"n", "+", "1"}]], "-", "1"}], 
        RowBox[{"x", "-", "1"}]], ")"}]}],
     TraditionalForm]}]}]],ExpressionUUID->
  "46e25ce7-28d7-4b83-ad50-fbcc614d6b46"],
 "\n\nWe want to comment on the following formula instead"
}], "Text",
 CellChangeTimes->{{3.7528333584854383`*^9, 3.752833403161993*^9}, {
   3.7528334978194075`*^9, 3.7528335245379353`*^9}, 3.752833791394199*^9, {
   3.752833907694851*^9, 3.7528339248088293`*^9}, {3.7528339577097116`*^9, 
   3.7528339928007183`*^9}, {3.7528340462147737`*^9, 3.752834344853855*^9}, {
   3.7528344026441603`*^9, 3.752834447818744*^9}, {3.7528344822507133`*^9, 
   3.7528346141722593`*^9}},ExpressionUUID->"08959d2d-ceba-4b46-8da3-\
aff528d77864"],

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"x", "=", "1"}], "n"], 
    RowBox[{"k", "*", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "x"], 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"x", "-", "t"}], ")"}], 
        RowBox[{"k", "-", "1"}]], 
       RowBox[{"\[DifferentialD]", "t"}]}]}]}]}], ",", 
   RowBox[{"{", 
    RowBox[{"k", ",", "1", ",", "5", ",", "1"}], "}"}]}], "]"}]], "Input",
